/dts-v1/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black";
	part-number = "jc_gpio_test";
	version = "00A0";

	fragment@0 {
		target = <&am33xx_pinmux>;

		__overlay__ {
			pinctrl_test: jc_gpio_test_pins {
				pinctrl-single,pins = <0x60 0x7 0xa8 0x37>;
			};
		};
	};

	fragment@1 {
		target = <&ocp>;

		__overlay__ {
			test_helper: helper {
				compatible = "bone-pinmux-helper";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_test>;
				status = "okay";
			};
		};
	};
};
